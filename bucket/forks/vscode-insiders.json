            },
            "arm64": {
                "url": "https://vscode.download.prss.microsoft.com/dbazure/download/insider/$matchCommit/VSCode-win32-arm64-$matchProduct.zip"
            }
        },
        "hash": {
            "url": "https://code.visualstudio.com/sha?build=insider",
            "regex": "(?s)$url.*?$sha256"
