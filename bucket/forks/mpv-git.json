{
    "version": "20231004",
    "description": "Video player based on MPlayer/mplayer2 (builds by shinchiro; AVX, nightly)",
    "homepage": "https://mpv.io",
    "license": "GPL-2.0-or-later OR LGPL-2.1-or-later",
    "notes": [
        "To set and unset file type associations and AutoPlay handlers, run '$dir\\installer\\mpv-install.bat' and '$dir\\installer\\mpv-uninstall.bat' as administrator, respectively.",
        "Optionally, you can install Icaros to enable thumbnails for all media types."
    ],
    "suggest": {
        "yt-dlp": "main/yt-dlp",
        "Icaros": "kazzarin/icaros-np"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/shinchiro/mpv-winbuild-cmake/releases/download/20231004/mpv-x86_64-v3-20231004-git-7b8a30f.7z",
            "hash": "be0775754db4626a0ff33832e5d9b360dfaa5f71e930a422439cddbf8fcb8162"
        }
    },
    "pre_install": "Remove-Item \"$dir\\updater.bat\"",
    "env_add_path": ".",
    "shortcuts": [
        [
            "mpv.exe",
            "mpv"
        ]
    ],
    "persist": "portable_config",
    "checkver": {
        "github": "https://github.com/shinchiro/mpv-winbuild-cmake",
        "jsonpath": "$..browser_download_url",
        "regex": "mpv-x86_64-v3-(\\d+)-git-(?<commit>[\\da-f]+)\\.7z"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/shinchiro/mpv-winbuild-cmake/releases/download/$version/mpv-x86_64-v3-$version-git-$matchCommit.7z"
            }
        }
    }
}
