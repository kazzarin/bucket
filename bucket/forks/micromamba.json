{
    "version": "1.5.0-0",
    "description": "Micromamba is a tiny version of mamba, the fast conda package installer.",
    "homepage": "https://mamba.readthedocs.io/en/latest/user_guide/micromamba.html",
    "license": "BSD-3-Clause",
    "architecture": {
        "64bit": {
            "url": "https://anaconda.org/conda-forge/micromamba/1.5.0/download/win-64/micromamba-1.5.0-0.tar.bz2",
            "hash": "3e54e468f3ff16194790570f4b356c7f379249571fddc7982753c88521c8062c"
        }
    },
    "extract_dir": "Library\\bin",
    "post_install": "Remove-Item \"$dir\\Library\" -Force",
    "env_set": {
        "MAMBA_ROOT_PREFIX": "$dir\\mamba"
    },
    "bin": [
        "micromamba.exe",
        [
            "micromamba.exe",
            "umamba"
        ]
    ],
    "persist": "mamba",
    "checkver": {
        "url": "https://api.anaconda.org/release/conda-forge/micromamba/latest",
        "regex": "win-64/micromamba-([\\d.-]+).tar.bz2"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://anaconda.org/conda-forge/micromamba/$matchHead/download/win-64/micromamba-$version.tar.bz2",
                "hash": {
                    "url": "https://api.anaconda.org/release/conda-forge/micromamba/$matchHead",
                    "regex": "(?sm)win-64/micromamba-$version.tar.bz2.*?$sha256"
                }
            }
        }
    }
}
