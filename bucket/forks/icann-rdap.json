    "installer": {
        "script": [
            "Get-ChildItem \"$dir\\rdap*\" -File | Where-Object { $_.Extension -eq '' } | ForEach-Object { Rename-Item $_.FullName \"$($_.FullName).exe\" }"
        ]
    },
    "bin": [
        "rdap.exe",
        "rdap-srv.exe",
        "rdap-srv-data.exe",
        "rdap-srv-store.exe",
        "rdap-srv-test-data.exe",
        "rdap-test.exe"
    ],
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/icann/icann-rdap/releases/download/v$version/icann-rdap-x86_64-pc-windows-msvc.tar.gz"
            }
        }
    }
